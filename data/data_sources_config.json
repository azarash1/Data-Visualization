{
    "project_info": {
        "description": "Data extraction configuration for Health & Economic Dashboard",
        "target_file": "data/real_data.json",
        "version": "1.0"
    },
    "time_range": {
        "start_year": 2000,
        "end_year": 2022,
        "frequency": "annual"
    },
    "regions": {
        "United States": [
            "Alabama",
            "Alaska",
            "Arizona",
            "Arkansas",
            "California",
            "Colorado",
            "Connecticut",
            "Delaware",
            "District of Columbia",
